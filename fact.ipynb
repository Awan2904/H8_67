    "def fact(n): \n",
    "    return 1 if n == 1 else n * fact(n-1)\n",
    "\n",
    "    if (__name__ == '__main__'): \n",
    "        import sys \n",
    "    if len(sys.argv) > 1: \n",
    "        print(fact(int(sys.argv[1])))"
   ]
